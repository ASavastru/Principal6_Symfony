{% extends 'base.html.twig' %}

{% block title %}Create A New Record | {{ parent() }}{% endblock %}

{% block body %}

<h1>{{ title }}</h1>

<div>
    Tracks:

    <ul>
        {% for track in tracks %}
            <li>
                {{ track.song }} - {{ track.artist }}
            </li>
        {% endfor %}
    </ul>
</div>

{% endblock %}

